{
  "name": "chrome-extension-webpack",
  "version": "1.0.0",
  "description": "ChromeExtension webpack4.x template for vue.js",
  "scripts": {
    "dev": "npm run lint && webpack --config build/webpack.dev.config.js",
    "build": "npm run lint && webpack --config build/webpack.prod.config.js",
    "lint": "eslint \"src/**/*.{js,vue}\" --fix & stylelint \"src/**/*.{css,scss,vue}\" --fix & exit 0"
  },
  "dependencies": {
    "@webcomponents/webcomponentsjs": "^2.3.0",
    "axios": "^0.21.1",
    "draggable-vue-directive": "^2.0.6",
    "vue": "^2.6.10",
    "webextension-polyfill": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "cache-loader": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "core-js": "^3.6.5",
    "css-loader": "^3.5.1",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^5.2.3",
    "fibers": "^4.0.2",
    "file-loader": "^4.3.0",
    "html-webpack-plugin": "^3.2.0",
    "http-serve": "^1.0.1",
    "lint-staged": "^9.5.0",
    "mini-css-extract-plugin": "^0.8.2",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-reporter": "^6.0.1",
    "sass": "^1.26.3",
    "sass-loader": "^7.3.1",
    "style-loader": "^1.1.3",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "url-loader": "^2.3.0",
    "vue-loader": "^15.9.1",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.42.1",
    "webpack-build-notifier": "^1.2.2",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-extension-reloader": "^1.1.4",
    "webpack-merge": "^4.2.2",
    "write-file-webpack-plugin": "^4.5.1",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -e"
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ],
    "src/**/*.{css,scss,vue}": [
      "stylelint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/molvqingtai/chrome-extension-webpack.git"
  },
  "keywords": [
    "chrome-extension",
    "chrome",
    "vue"
  ],
  "author": "molvqingtai",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/molvqingtai/chrome-extension-webpack/issues"
  },
  "homepage": "https://github.com/molvqingtai/chrome-extension-webpack#readme"
}
